<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">

  <Id>2f6d2f52-1d54-4f6b-9b6c-123456789abc</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Microsoft Test</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Microsoft Test Add-in"/>
  <Description DefaultValue="Minimal test Outlook add-in."/>

  <IconUrl DefaultValue="https://appsforoffice.microsoft.com/lib/1/images/icon32.png"/>
  <HighResolutionIconUrl DefaultValue="https://appsforoffice.microsoft.com/lib/1/images/icon64.png"/>
  <SupportUrl DefaultValue="https://support.microsoft.com"/>

  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>

  <FormSettings>
    <Form xsi:type="ItemRead">
      <WebSettings>
        <SourceLocation DefaultValue="https://appsforoffice.microsoft.com/lib/1/hosted/OutlookAppRead.html"/>
        <RequestedHeight>250</RequestedHeight>
      </WebSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadItem</Permissions>

</OfficeApp>
